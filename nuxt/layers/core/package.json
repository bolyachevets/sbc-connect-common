{
  "name": "@daxiom/nuxt-core-layer-test",
  "type": "module",
  "version": "0.0.25",
  "main": "./nuxt.config.ts",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "nuxi dev .playground --port 3000",
    "dev:prepare": "nuxt prepare .playground",
    "generate": "nuxt generate .playground",
    "preview": "nuxt preview .playground",
    "postinstall": "nuxt prepare",
    "lint": "eslint --ext \".ts,.vue\" --ignore-path .gitignore",
    "lint:fix": "pnpm lint --fix",
    "test": "vitest --dir tests/unit --passWithNoTests",
    "test:unit": "vitest --dir tests/unit",
    "test:unit:cov": "vitest run --dir tests/unit --coverage",
    "publish": "node scripts/publish.js"
  },
  "devDependencies": {
    "@nuxt/test-utils": "^3.18.0",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@testing-library/vue": "^8.1.0",
    "@types/jsdom": "^21.1.7",
    "@typescript-eslint/parser": "^8.18.2",
    "@vitest/coverage-v8": "3.1.3",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^8.56.0",
    "eslint-plugin-tailwindcss": "^3.17.5",
    "happy-dom": "^17.4.6",
    "nuxt": "3.15.4",
    "ora": "^8.1.1",
    "typescript": "^5.7.2",
    "vitest": "^3.1.3",
    "vue": "^3.5.0",
    "vue-router": "^4.5.1"
  },
  "dependencies": {
    "@iconify-json/mdi": "^1.2.2",
    "@nuxt/ui": "^2.20.0",
    "@nuxtjs/i18n": "^8.5.6",
    "@pinia/nuxt": "^0.11.0",
    "@vueuse/nuxt": "^13.1.0",
    "dompurify": "^3.2.3",
    "jsdom": "^26.1.0",
    "keycloak-js": "^26.0.7",
    "launchdarkly-vue-client-sdk": "^2.3.0",
    "maska": "^3.0.4",
    "pinia": "^3.0.2",
    "pinia-plugin-persistedstate": "^4.2.0",
    "zod": "^3.24.1"
  }
}
